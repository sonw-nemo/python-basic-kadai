      "source": [
        "# 台形の面積　=　（上辺　+　下辺）×高さ　÷　2\n",
        "top = 10    #上辺（cm）\n",
        "bottom = 20 #下辺（cm）\n",
        "height =5   #高さ（cm）\n",
        "\n",
        "#面積の計算\n",
        "area = (top +bottom )* height / 2\n",
        "\n",
        "#　結果を出力\n",
        "print(f\"{area}cm²\")"
      ]
    }
  ]
